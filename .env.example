# OpenAI API Key (if needed for any features)
OPENAI_API_KEY=your_openai_api_key_here

# App Secret Key - used for securing sessions
SECRET_KEY=your_super_secret_key_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple Sign In Configuration
APPLE_CLIENT_ID=your_apple_client_id_here
APPLE_CLIENT_SECRET=your_apple_client_secret_here
# Use your Render URL in production, e.g. https://your-app.onrender.com/auth/apple/callback
APPLE_REDIRECT_URI=https://your-domain.com/auth/apple/callback